{
	"name": "Finish.it",
	"version": "1.0",
	"description": "Be more productive!",
	"permissions": ["storage", "tabs", "webNavigation", "storage", "http://*/", "https://*/"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["urlmagic.js"],
			"run_at": "document_start"
		}
	],
	"manifest_version": 2
}
