{
	"name": "Finish.it",
	"version": "0.0001",
	"description": "Be more productive!",
	"permissions": ["storage", "tabs", "webNavigation", "storage", "http://*/", "https://*/"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"32": "assets/img/32icon.png"
		}
	},

	"manifest_version": 2
}
